{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
   },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
    ],
  "routes": [
    {
      "src": "/task/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
      },
    {
      "src": "/(.+)",
      "dest": "public/$1"
    }
    ]
}


